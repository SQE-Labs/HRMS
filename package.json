{
  "name": "hrmis-cypress-project",
  "version": "1.0.0",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run:chrome": "cypress run --browser chrome  --spec 'cypress/e2e/tests/*.cy.js'",
    "cypress:run:chrome:headed": "cypress run --browser chrome --headed",
    "test": "npm run cypress:run:chrome",
    "test:headed": "npm run cypress:run:chrome:headed",
    "parallel": "cypress-parallel -s cypress/e2e/tests/**/*.cy.js -t 4",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "test:allure": "npx cypress run --env allure=true",
    "allure:report": "allure generate \"allure-results\" --clean -o \"allure-report\" && allure open \"allure-report\"",
    "merge:report": "npx mochawesome-merge cypress/reports/*.json -o cypress/reports/merged.json && npx marge cypress/reports/merged.json --reportDir cypress/reports --inline --reportFilename mochawesome.html --reportTitle 'HRMS Test Execution Report'",
    "cy:run": "cypress run"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "allure-commandline": "^2.34.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "chai": "^5.1.2",
    "cypress": "^15.6.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-mochawesome-reporter": "3.8.2",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-parallel": "^0.14.0",
    "cypress-split": "^1.24.6",
    "cypress-xpath": "^2.0.1",
    "easy-yopmail": "5.1.0",
    "https-browserify": "^1.0.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.2",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "util": "^0.12.5",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "assert": "^2.1.0",
    "browserify-fs": "^1.0.0",
    "mocha": "^8.2.1"
  }
}
